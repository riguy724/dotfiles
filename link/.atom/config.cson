"*":
  "atom-terminal":
    MacWinRunDirectly: true
  core:
    disabledPackages: [
      "flatten-json"
    ]
    packagesWithKeymapsDisabled: [
      "nerd-treeview"
      "open-on-github"
    ]
  editor:
    invisibles: {}
  "exception-reporting":
    userId: "39a1dca6-432e-ae78-da0b-e2b93d7a04cc"
  "fuzzy-finder": {}
  "gist-it": {}
  "git-plus": {}
  linter: {}
  "linter-pep8":
    pep8ExecutablePath: "/usr/local/bin/pep8"
  "linter-pylint": {}
  minimap:
    plugins:
      codeglance: true
      "find-and-replace": true
      "git-diff": true
    textOpacity: 0.8
  "open-on-github":
    includeLineNumbersInUrls: false
  "open-terminal-here": {}
  preview: {}
  "symbols-tree-view":
    autoHide: true
  tabs:
    usePreviewTabs: true
  "tree-view-filter":
    liveUpdate: true
  welcome:
    showOnStartup: false
